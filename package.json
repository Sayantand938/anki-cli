    "cli-table3": "^0.6.5",
    "clipboardy": "^4.0.0",
    "commander": "^14.0.0",
    "env-paths": "^3.0.0",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.1",
    "globby": "^14.1.0",
    "inquirer": "^12.9.1",
    "rimraf": "^6.0.1",
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
