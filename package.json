{
  "name": "anki-cli",
  "version": "1.0.0",
  "type": "module",
  "description": "A simple CLI tool",
  "main": "dist/cli.js",
  "bin": {
    "anki-cli": "dist/cli.js"
  },
  "scripts": {
    "start": "node dist/cli.js",
